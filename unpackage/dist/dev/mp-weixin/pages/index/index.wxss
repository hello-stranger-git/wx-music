.content .search {
  height: 70rpx;
  padding: 20rpx 24rpx 20rpx 24rpx;
  background: #FFFFFF;
  position: relative;
  z-index: 1;
}
.content .searchIcon {
  position: absolute;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  left: 48rpx;
}
.content .uni-input {
  height: 100%;
  border-radius: 36rpx;
  background: #F7F7F7;
  padding-left: 72rpx;
}
.content .searchMusicData {
  height: 750rpx;
  overflow-y: auto;
  border-radius: 18rpx;
  margin-top: 20rpx;
  background: rgba(212, 212, 212, 0.8);
}
.content .searchMusicData ._li {
  padding: 32rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.content .searchMusicData ._li .musicName {
  -webkit-box-flex: 3;
  -webkit-flex: 3;
          flex: 3;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.content .searchMusicData ._li .userName {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: right;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.content .swiper {
  height: 750rpx;
}
.content .swiper image {
  width: 100%;
}

